@{
}

<div>
    @foreach (var user in ViewBag.User)
    {
        <div class="container">
            <div class="user">
                <span>@user.Login</span>
                <span>@user.Password</span>
                <span>@user.FullName</span>
                <span>@user.Id</span>
            </div>
        </div>
    }
</div>